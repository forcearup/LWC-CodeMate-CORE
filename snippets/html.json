            "$2"
        ],
        "description": "LWC Button Icons Control. (ROY-LWC CodeMate)"
	},
	"roy.lwc.button-icon-stateful": {
        "prefix": "roy.lwc.button-icon-stateful",
        "body": ["<lightning-button-icon-stateful icon-name=\"utility:like\" size=\"xx-small\" selected={likeStateSize01} onclick={handleLikeButtonSizeClick} alternative-text=\"Like\" data-button-number=\"01\"></lightning-button-icon-stateful>",
                "  <!-- size attribute (xx-small, x-small, small, medium)  -->  ",
            "$2"
        ],
        "description": "LWC Button Icon Statefull Control. (ROY-LWC CodeMate)"
	},
	"roy.lwc.button-menu-with-iterations": {
        "prefix": "roy.lwc.button-menu-with-iterations",
        "body": ["<lightning-button-menu alternative-text=\"Show menu\" variant=\"border-filled\" menu-alignment=\"auto\">",
		"   <template for:each={myMenuItems} for:item=\"menuItem\">",
		"       <lightning-menu-item",
		"       key={menuItem.id}",
		"       value={menuItem.value}",
		"       label={menuItem.label}>",
		"       </lightning-menu-item>",
		"    </template>",
	    "</lightning-button-menu>",
            "$2"
        ],
        "description": "LWC Button menu with iterations Control. (ROY-LWC CodeMate)"
	},
	"roy.lwc.button-stateful": {
        "prefix": "roy.lwc.button-stateful",
        "body": ["<lightning-button-stateful",
        "    label-when-off=\"Follow\"",
        "    label-when-on=\"Following\"",
        "    label-when-hover=\"Unfollow\"",
        "    icon-name-when-off=\"utility:add\"",
        "    icon-name-when-on=\"utility:check\"",
        "    icon-name-when-hover=\"utility:close\"",
        "    selected={isSelected}",
        "    onclick={handleClick}>",
    "</lightning-button-stateful>",
            "$2"
        ],
        "description": "LWC Button Statefull Control. (ROY-LWC CodeMate)"
	},
